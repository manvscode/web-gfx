  "homepage": "https://bitbucket.org/manvscode/web-gfx#readme",
  "dependencies": {
    "serve": "^13.0.2"
  },
  "devDependencies": {
    "server": "^1.0.37",
    "traceur": "0.0.111",
    "zip-dir": "^2.0.0"
  }
}
